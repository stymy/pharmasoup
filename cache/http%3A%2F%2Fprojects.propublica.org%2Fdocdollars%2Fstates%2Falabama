    <script>
        $('#limit_checkbox_input').click( function(){
            // Immediately disable the checkbox b/c of the small lag between interaction and execution
            $('#limit_checkbox_input').attr('disabled', true);
            var w = window.location.href;

            // If user checks the box, remove showall=true
            if ($('#limit_checkbox_input').is(':checked')){
              window.location = w.replace('&showall=true', '').replace('?showall=true', '');

            // Else if user unchecks the box, add it
            } else {
              if (w.indexOf('?') == -1){
                  window.location = w + "?showall=true";
              } else {
                  window.location = w + "&showall=true";
              }
            }
        })
    </script>

